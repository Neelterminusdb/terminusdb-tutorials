[
    {"@type" : "@context",
     "@base" : "http://lib.terminusdb.com/units/",
     "@schema" : "http://lib.terminusdb.com/units#",
     "@documentation" :
     { "@title" : "Dimensional Analysis Library",
       "@description" : "An importable library describing units for dimensional analysis.",
       "@authors" : ["Gavin Mendel-Gleason"]
     }
    },

    { "@type" : "Class",
      "@id" : "Quantity",
      "@subdocument" : [],
      "unit" : "Unit",
      "quantity" : "xsd:decimal" },

    { "@type" : "Class",
      "@id" : "Unit",
      "dimension" : "Dimension",
      "name" : "xsd:string",
      "plural" : { "@type" : "Optional",
                   "@class" : "xsd:string" },
      "alternative_name" : { "@type" : "Set",
                             "@class" : "xsd:string"},
      "derived_from" : { "@type" : "Optional",
                         "@class" : "xsd:string" }},

    { "@type" : "Enum",
      "@id" : "Dimension",
      "@value" : [
          "currency",
          "time",
          "length",
          "mass",
          "area",
          "space",
          "temperature",
          "energy",
          "power",
          "force",
          "torque",
          "speed",
          "rotational_speed",
          "acceleration",
          "charge",
          "electric_potential",
          "electric_current",
          "eletcric_resistance",
          "momentum",
          "angular_momentum",
          "dimensionless"
      ]
    }

]
